<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.climbon">

    <application
        android:name=".ClimbOnApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ClimbOn">
        <activity
            android:name=".BrowseRoutesActivity"
            android:exported="false" />
        <activity
            android:name=".ChooseWallActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateWallActivity"
            android:exported="false" />
        <activity
            android:name=".SetHoldsActivity"
            android:exported="false"
            android:label="@string/title_activity_set_holds"
            android:theme="@style/Theme.ClimbOn.NoActionBar" />
        <activity
            android:name=".RouteView"
            android:exported="false" />
        <activity
            android:name=".PanelView"
            android:exported="false" />
        <activity
            android:name=".MainMenu"
            android:exported="false" />
    </application>

</manifest>